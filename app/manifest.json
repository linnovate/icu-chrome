{
  "name": "__MSG_appName__",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.linnovate.net/*",
        "https://www.linnovate.net/*",
        "http://www.ynet.co.il/*"
      ],
      "css": ["lib/sweetalert/sweetalert.css"],
      "js": [
        "lib/sweetalert/sweetalert.min.js",
        "scripts/contentscript.js",
        "scripts/settings.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "identity"
  ],
  "oauth2": {
    "client_id": "473899478840-g913bbp5p8jcso3qa30cbtac10j21t08.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
